 <div id="preloader">
        <div class="sk-three-bounce">
            <div class="sk-child sk-bounce1"></div>
            <div class="sk-child sk-bounce2"></div>
            <div class="sk-child sk-bounce3"></div>
        </div>
    </div>

    <div id="main-wrapper">

        {{>admin-header}}
	    {{>admin-sideNavbar}}
        <div class="content-body">
            <div class="container-fluid">
				<!-- Add Order -->
				
                <div class="page-titles">
					<ol class="breadcrumb">
						<li class="breadcrumb-item active"><a href="javascript:void(0)">Users</a></li>
					</ol>
                </div>
                <!-- row -->
                <div class="row">
					<div class="col-12">
                        <div class="card">
                            <div class="card-header">
                                <h4 class="card-title">Users Table</h4>
                            </div>
                            <div class="card-body">
                                <div class="table-responsive">
                                    <table id="example4" class="display min-w850">
                                        <thead>
                                            <tr>
                                                <th>No</th>
                                                <th>Name</th>
                                                <th>Email</th>
                                                <th>Mobile</th>
                                                <th>Date</th>
												<th>action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            {{#each users}}
                                            <tr>
                                                <td>{{@index}}</td>
                                                <td>{{this.name}}</td>
                                                <td>{{this.email}}</td>
                                                <td>{{this.mobile}}</td>
                                                <td>{{this.date}}</td>
                                                <td>
                                                    {{#if this.state}}
                                                                                                        
                                                    {{!-- <a  href="/admin/block-users/{{this._id}}" class="badge light badge-success" data-toggle="modal" data-target="#myModal">Active</a> --}}

                                                    <a href="/admin/block-users/{{this._id}}" class="badge light badge-success" onclick="return confirm('Are you sure you want to Block user?')" >Active </a>
                                                     {{else}}
                                                    
                                                    {{!-- <a  href="/admin/unblock-user/{{this._id}}" class="badge light badge-danger" data-toggle="modal" data-target="#myModal">Block</a> --}}
                                                    <a href="/admin/unblock-user/{{this._id}}" class="badge light badge-danger" onclick="return confirm('Are you sure you want to Active user?')"  >Block </a>
                                                    {{/if}}
                                                    </td>
                                            </tr>
                                            {{/each}}
                                      </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
				</div>
            </div>
        </div>

        
           {{#each users}}
            <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabelLogout" aria-hidden="true">
            <div class="modal-dialog" role="document">
              <div class="modal-content">
                <div class="modal-header">
                  <h5 class="modal-title" id="exampleModalLabelLogout">Ohh No!!!!</h5>
                  <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                  </button>
                </div>
                <div class="modal-body">
                     {{#if this.state}}
                  <p>Are you sure you want to Block the user?</p>
                  {{else}}
                  <p>Are you sure you want to Unblock the user?</p>
                  {{/if}}
                </div>
                <div class="modal-footer">
                  <button type="button" class="btn btn-outline-primary" data-dismiss="modal">Cancel</button>
                  {{#if this.state}}
                  <a href="/admin/block-users/{{this._id}}" class="btn btn-danger">Block</a>
                  {{else}}
                    <a href="/admin/unblock-user/{{this._id}}" class="btn btn-success">UNBlock</a>
                   {{/if}}
                </div>
              </div>
            </div>
          </div>
           {{/each}}






    </div>

    <script src="/adminSide/assets/vendor/global/global.min.js"></script>
    <!-- Datatable -->
    <script src="/adminSide/assets/vendor/datatables/js/jquery.dataTables.min.js"></script>
    <script src="/adminSide/assets/js/plugins-init/datatables.init.js"></script>

