									<div class="card-body">
										<div class="d-flex justify-content-between align-items-center">
											<div>
                                                <p><b>Bill Amout</b></p>
												<p class="fs-14 mb-1">Total Amout : <span>{{../singleTotal.[0].total}}</span>   </p>
											</div>
										</div>
									</div>
