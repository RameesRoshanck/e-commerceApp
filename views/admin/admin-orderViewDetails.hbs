				<!-- Add Order -->
				
                <div class="page-titles">
					<ol class="breadcrumb">
						<li class="breadcrumb-item active"><a href="#">Orderss</a></li>
					</ol>
                </div>
                <!-- row -->
                <div class="row">
					<div class="col-12">
                        <div class="card">
                            <div class="card-header">
                                <h4 class="card-title">Products Table</h4>
                            </div>
                            <div class="card-body">
                                <div class="table-responsive">
                                    <table id="example4" class="display min-w850">
                                        <thead>
                                            <tr>
                                                <th>Sl No:</th>
                                                <th>image</th>
                                                <th>Name</th>
                                                <th>price</th>
                                                <th>Qunatity</th>
                                                <th>offer</th>
                                                <th>Total Amount</th>
                                                <th>Status</th>
                                          {{!-- <th>Action</th> --}}
                                               
                                            </tr>
                                        </thead>
                                        <tbody>
                                             {{#each singleorder}} 
                                                <tr>
                                                <td>{{@index}}</td>
                                                        <td><img style="width:110px;height:130px"
                                                            src="/product-images/{{this.product.image.[0]}}" alt="pic"></td>
                                                <td>{{this.product.name}}</td>
                                                <td>{{this.product.price}}</td>
                                                <td>{{this.quantity}}</td>
                                                <td>{{this.product.offer}}</td>
                                                <td>{{this.singleTotal}}</td>
                                              
                                                <td>{{../orderAddress.status}}</td>
                                             
                                              </tr>
                                              {{/each}}
                                       </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
				</div>
            </div>
        </div>
    </div>

