 <div id="preloader">
        <div class="sk-three-bounce">
            <div class="sk-child sk-bounce1"></div>
            <div class="sk-child sk-bounce2"></div>
            <div class="sk-child sk-bounce3"></div>
        </div>
    </div>

    <div id="main-wrapper">

        {{>admin-header}}
	    {{>admin-sideNavbar}}



        <div class="content-body">
            <div class="container-fluid">
				<!-- Add Order -->
				
                <div class="page-titles">
					<ol class="breadcrumb">
						<li class="breadcrumb-item active"><a href="javascript:void(0)">Banner Management</a></li>
					</ol>
                </div>
              <div class="col-xl-6 col-lg-12">
                        <div class="card">
                            <div class="card-header">
                                <h4 class="card-title">Add Banner</h4>
                            </div>
                            <div class="card-body">
                                <div class="basic-form">
                                     <form action="/admin/addBanner" method="post" enctype="multipart/form-data" >
                                        <div class="form-row">
                                            <div class="form-group">
                                                <label>Banner Name</label>
                                                <input type="text" class="form-control" placeholder="entet the Brand name" name="name" >
                                            </div>
                                           <div class="form-group">
                                                 <label>Choose Banner Image File</label>
                                                 <input multiple type="file" id="product_images[]" accept="image/*r" name="image" class="form-control" >
                                            </div>
                                        </div>
                                        <button type="submit" class="btn btn-primary">ADD Banner</button>
                                    </form>
                                </div>
                            </div>
                        </div>
					</div>
                <!-- row -->
                <div class="row">
					<div class="col-12">
                        <div class="card">
                            <div class="card-header">
                                <h4 class="card-title">Brand Table</h4>
                            </div>
                            <div class="card-body">
                                <div class="table-responsive">
                                    <table id="example4" class="display min-w850">
                                        <thead>
                                            <tr>
                                                <th>No</th>
                                                <th>Images</th>
                                                <th></th>
                                                <th>Brands</th>
                                                <th>Edit</th>
												<th>action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            {{#each Banner}}
                                            <tr>
                                                <td>{{@index}}</td>
                                                <td><img style="width:50px;,height:50px ;" src="/product-images/{{this.image.[0]}}" alt=""> </td>
                                                <td><img style="width:50px;,height:50px ;" src="/product-images/{{this.image.[1]}}" alt=""> </td>
                                                <td>{{this.name}}</td>
                                                <td><a  href="/admin/editBanner/{{this._id}}"class="btn btn-danger shadow btn-xs sharp"><i class="fa fa-pencil"></a></td> 
                                                <td><a  href="/admin/deleteBanner/{{this._id}}" onclick="return confirm('Are you sure you want to delete {{this.name}}?')" class="btn btn-danger shadow btn-xs sharp"><i class="fa fa-trash"></i></a></td>
                                            </tr>
                                          {{/each}}
                                      </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
				</div>
            </div>
        </div>
    </div>
     





